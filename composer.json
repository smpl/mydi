{
  "name": "smpl/mydi",
  "description": "My small dependency injection implementation",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "license": "GNUv2",
  "authors": [
    {
      "name": "Evgeniy Kuvshinov",
      "email": "smpl@itmywork.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "smpl\\mydi\\": [
        "src/"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "smpl\\mydi\\test\\": "tests/",
      "smpl\\mydi\\example\\": "example/"
    }
  },
  "require": {
    "php": ">=5.5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "@stable"
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}