{
  "name": "smpl/mydi",
  "description": "psr/container implementation for humans",
  "keywords": [
    "psr-11",
    "psr container",
    "di",
    "dependency injection",
    "container"
  ],
  "license": "GPL-3.0",
  "authors": [
    {
      "name": "Evgeniy Kuvshinov",
      "email": "smpl@itmywork.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Smpl\\Mydi\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Smpl\\Mydi\\Test\\": "test/"
    }
  },
  "require": {
    "php": ">=7",
    "ext-json": "*",
    "psr/cache": "^1.0",
    "psr/simple-cache": "^1.0",
    "psr/container": "^1.0"
  },
  "provide": {
    "psr/container-implementation": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "@stable",
    "squizlabs/php_codesniffer": "@stable"
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "phpcs": "vendor/bin/phpcs"
  }
}
